    "# import all the needed libraries\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# define all the constants\n",
    "hbar = 1\n",
    "m = 1\n",
    "alpha = 1\n",
    "Lambda = 4"
   ]
